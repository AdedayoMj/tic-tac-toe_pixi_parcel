    "minimist": ">=1.2.2",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "path-browserify": "^1.0.1",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^3.8.3",
    "url": "^0.11.0"
  },
  "dependencies": {
    "@colyseus/command": "^0.1.6",
    "@colyseus/monitor": "^0.12.2",
